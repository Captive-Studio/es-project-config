{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>Captive-Studio/renovate-config:library"],
  "packageRules": [
    {
      "matchPackageNames": ["vue"],
      "matchUpdateTypes": ["patch", "minor"],
      "automerge": true
    },
    {
      "matchPackagePatterns": ["@w5s/"],
      "matchUpdateTypes": ["patch", "minor"],
      "automerge": true
    }
  ]
}
