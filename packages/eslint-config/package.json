{
  "name": "@captive/eslint-config",
  "version": "1.1.0",
  "description": "ESLint configuration presets",
  "keywords": [
    "eslint",
    "config",
    "lint"
  ],
  "homepage": "https://github.com/Captive-Studio/es-project-config/blob/master/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/Captive-Studio/es-project-config.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Captive-Studio/es-project-config.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@captive.fr>",
  "main": "index.js",
  "files": [
    "*.js",
    "rules/*.js"
  ],
  "scripts": {
    "build": "npm-run-all -p 'build:*'",
    "build:empty": ":",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "is-ci || scripts/pull",
    "test": "scripts/test"
  },
  "eslintConfig": {
    "rules": {
      "prefer-object-spread": "off",
      "sort-keys-fix/sort-keys-fix": "error"
    }
  },
  "eslintIgnore": [
    "__tests__"
  ],
  "dependencies": {
    "@babel/eslint-parser": "^7.0.0",
    "@babel/plugin-syntax-class-properties": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsdoc": "^37.0.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unicorn": "^37.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.0.0",
    "@babel/plugin-syntax-class-properties": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@types/eslint": "7.2.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/prettier": "2.1.6",
    "@types/react": "^16.9.34",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-index": "1.5.0",
    "prettier": "2.5.1",
    "react": "17.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "__peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "eslint": "5.x || 6.x || 7.x || 8.x",
    "eslint-plugin-functional": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsdoc": "^37.0.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-total-functions": "^5.0.0",
    "eslint-plugin-unicorn": "^37.0.0",
    "prettier": "2.x"
  }
}
