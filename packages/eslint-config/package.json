{
  "name": "@captive/eslint-config",
  "version": "1.7.11",
  "description": "ESLint configuration presets",
  "keywords": [
    "eslint",
    "config",
    "lint"
  ],
  "homepage": "https://github.com/Captive-Studio/es-project-config/blob/master/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/Captive-Studio/es-project-config.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Captive-Studio/es-project-config.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@captive.fr>",
  "type": "commonjs",
  "main": "index.js",
  "files": [
    "lib/**/!(*.spec).d.ts",
    "lib/**/!(*.spec).js.map",
    "lib/**/!(*.spec).js",
    "src/**/!(*.spec).ts",
    "!example/**/*",
    "!*.cjs",
    "index.js",
    "index.d.ts",
    "*.js",
    "rules/*.js"
  ],
  "scripts": {
    "build": "concurrently \"npm:build:*\" \":\"",
    "build:tsc": "tsc",
    "clean": "concurrently \"npm:clean:*\" \":\"",
    "clean:src": "rm -rf lib/*",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "concurrently \"npm:format:*\" \":\"",
    "format:src": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "lint": "concurrently \"npm:lint:*\" \":\"",
    "lint:src": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json,jsonc,json5,yml,yaml",
    "prepare": "concurrently \"npm:prepare:*\" \":\"",
    "prepublishOnly": "npm run clean;npm run build",
    "spellcheck": "cspell --no-progress '**'",
    "test": "concurrently \"npm:test:*\" ",
    "test:src": "scripts/test",
    "test_:rule-coverage": "eslint-find-rules --unused"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules",
      "_tests"
    ]
  },
  "dependencies": {
    "@captive/prettier-config": "^1.1.5",
    "@w5s/eslint-config": "^1.0.0-alpha.47",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0"
  },
  "devDependencies": {
    "@types/eslint": "8.4.10",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "29.2.5",
    "@types/prettier": "2.7.2",
    "@types/react": "18.0.26",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-find-rules": "4.1.0",
    "prettier": "2.8.3",
    "react": "18.2.0",
    "vue": "3.2.45"
  },
  "peerDependencies": {
    "eslint": "8.x",
    "typescript": "4.x"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
