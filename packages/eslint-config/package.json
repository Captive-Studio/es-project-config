{
  "name": "@captive/eslint-config",
  "version": "1.2.4",
  "description": "ESLint configuration presets",
  "keywords": [
    "eslint",
    "config",
    "lint"
  ],
  "homepage": "https://github.com/Captive-Studio/es-project-config/blob/master/packages/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/Captive-Studio/es-project-config.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Captive-Studio/es-project-config.git",
    "directory": "packages/eslint-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@captive.fr>",
  "main": "index.js",
  "files": [
    "*.js",
    "rules/*.js"
  ],
  "scripts": {
    "build": "concurrently 'npm:build:*'",
    "build:empty": ":",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "pull-external": "scripts/pull",
    "spellcheck": "cspell --no-progress '**'",
    "test": "scripts/test"
  },
  "eslintConfig": {
    "rules": {
      "prefer-object-spread": "off",
      "sort-keys-fix/sort-keys-fix": "error"
    }
  },
  "eslintIgnore": [
    "__tests__"
  ],
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.0.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-vue": "^9.3.0"
  },
  "devDependencies": {
    "@types/eslint": "7.2.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/prettier": "2.1.6",
    "@types/react": "^16.9.34",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-index": "^1.5.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "eslint": "8.x",
    "prettier": "2.x",
    "typescript": "4.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
