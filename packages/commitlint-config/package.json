{
  "name": "@captive/commitlint-config",
  "version": "1.0.2",
  "description": "Commitlint configuration presets",
  "keywords": [
    "commitlint",
    "git",
    "lint"
  ],
  "homepage": "https://github.com/Captive-Studio/es-project-config/blob/master/packages/commitlint-config#readme",
  "bugs": {
    "url": "https://github.com/Captive-Studio/es-project-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Captive-Studio/es-project-config.git",
    "directory": "packages/commitlint-config"
  },
  "license": "MIT",
  "author": "Julien Polo <julien.polo@captive.fr>",
  "main": "commitlint.config.js",
  "files": [
    "commitlint.config.js"
  ],
  "scripts": {
    "build": "concurrently 'npm:build:*'",
    "build:empty": ":",
    "docs": "md-magic --path '**/*.md' --ignore='node_modules'",
    "format": "eslint . --fix --ext=mjs,cjs,js,jsx,ts,tsx,json",
    "lint": "eslint . --ext=mjs,cjs,js,jsx,ts,tsx,json",
    "spellcheck": "cspell --no-progress '**'",
    "test": "exit 0",
    "test:git-log": "cd ../..;npm exec commitlint --from HEAD~3 --to HEAD --verbose"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^13.0.0"
  },
  "peerDependencies": {
    "@commitlint/cli": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
